<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bing.mapper.GradeMapper">
    <select id="selectAll" resultType="com.bing.pojo.Grade">
        select * from grade
    </select>

    <select id="selectOne" resultType="com.bing.pojo.Grade">
        select * from grade where gradeId=#{gradeId}
    </select>
</mapper>