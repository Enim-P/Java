<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bing.mapper.AdminMapper">
    <select id="selectByLoginId" resultType="com.bing.pojo.Admin">
        select * from `admin` where loginId=#{loginId}
    </select>
</mapper>